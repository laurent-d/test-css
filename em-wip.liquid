    <!-- Editbale generaux -->
    {% capture background_banner %}{% editable_file "Bannière", priority: 99 %}{% endeditable_file %}{% endcapture %}
    {% capture lien_bouton_banner %}{% editable_text "Lien bouton sur bannière", priority: 99 %}#{% endeditable_text %}{% endcapture %}
    {% capture screenshot_banner %}{% editable_file "Screenshot sur bannière", priority: 99 %}{% endeditable_file %}{% endcapture %}
    {% capture facebook %}{% editable_text "Lien Facebook", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture twitter %}{% editable_text "Lien Twitter", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture googleplus %}{% editable_text "Lien Google+", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture linkedin %}{% editable_text "Lien Linkedin", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture rss %}{% editable_text "Lien RSS", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture youtube %}{% editable_text "Lien YouTube", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture instagram %}{% editable_text "Lien Instagram", priority: 20 %}{% endeditable_text %}{% endcapture %}
    {% capture copyright %}{% editable_text "Copyright", priority: 15 %}&copy; Copyright 2016 - 2020 {% endeditable_text %}{% endcapture %}
    {% capture text_cookie %}{% editable_text "Texte bannière Cookie ", priority: 15 %}Nous utilisons des cookies pour améliorer votre expérience {% endeditable_text %}{% endcapture %}
    {% capture text_lien_cookie %}{% editable_text "Texte Lien bannière Cookie ", priority: 15 %} en savoir plus {% endeditable_text %}{% endcapture %}
    {% capture lien_cookie %}{% editable_text "Lien bannière Cookie ", priority: 15 %} donnees-personnelles {% endeditable_text %}{% endcapture %}
    <!-- Editable FR -->
    {% capture title_banner %}{% editable_text "Titre sur bannière", priority: 98 %}Perfect choice for your web and mobile apps.{% endeditable_text %}{% endcapture %}
    {% capture text_banner %}{% editable_text "Texte sur bannière", priority: 98 %}Create a website that will impress your clients & visitors{% endeditable_text %}{% endcapture %}
    {% capture text_bouton_banner %}{% editable_text "Texte bouton sur bannière", priority: 98 %}Try{% endeditable_text %}{% endcapture %}
    {% capture text_footer %}{% editable_text "Texte footer", priority: 25, type: cktext_area %}<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Donec pellentesque efficitur turpis, vitae dictum dolor tristique in.</p>{% endeditable_text %}{% endcapture %}
    {% capture title_reference %}{% editable_text "Titre section Références", priority: 50 %}Our worldwide partners.{% endeditable_text %}{% endcapture %}
    {% capture text_reference %}{% editable_text "Texte section Références", priority: 49, type: cktext_area %}<p>Over 500,000 Customers worldwide</p>{% endeditable_text %}{% endcapture %}
    <style>.hero-static-image {background: url('{{background_banner}}') no-repeat;background-size: cover;}</style>
