  <body>
    <div id="preloader"><div id="preloader-inner"></div></div>
    <!--/preloader-->
    <!--back to top-->
    <a href="#" class="scrollToTop"><i class="ion-ios-arrow-up"></i></a>
    <!--back to top end-->
    <!-- Static navbar -->
    <nav class="navbar navbar-toggleable-md navbar-light fixed-top  navbar-transparent clearfix">
      <div class="clearfix"></div>
      <div class="container">
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <a class="navbar-brand" href="/home">
          <img data-src="{{ 'eventmaker_blanc_transparent.png' | image_url }}" alt="Eventmker logo" class="logo logo-default">
          <img data-src="{{ 'eventmaker_noir.png' | image_url }}" alt="Eventmaker logo" class="logo logo-scroll">
        </a>
        <div class="collapse navbar-collapse" id="navbarNavDropdown">
          <ul class="navbar-nav ml-auto">
            {% for item in menus.menu_principal %}
              {% assign item_url = item.url | strip | remove: "/" %}
              {% assign page_path = page.path_name | strip %}
              <li class="nav-item">
                {% if item.name == 'Blog' %}
                  <a class="nav-item-mobil {% if page_path == item_url %}nav-item-current{% endif %} nav-link" href="{{item.url}}" target="_blank" title="{{item.name}}">{{ item.name }}</a>
                {% elsif item.name == 'Pricing' %}
                  <a class="nav-item-mobil {% if page_path == item_url %}nav-item-current{% endif %} nav-link" href="/#pricing" title="{{item.name}}">{{ item.name }}</a>
                {% else %}
                  <a class="nav-item-mobil {% if page_path == item_url %}nav-item-current{% endif %} nav-link" href="{{item.url}}" title="{{item.name}}">{{ item.name }}</a>
                {% endif %}
              </li>
            {% endfor %}
            {%  comment %}
            <li class="nav-item">
            <a class="nav-item-mobil nav-link" href="{{baseurl}}/en" title="English">EN</a>
            </li>
            {% endcomment%}
            <li class="nav-item">
            
            <a class="nav-item-mobil nav-link btn btn-primary btn-rounded cta-demo" href="{{baseurl}}/demo" title="Découvrir notre solution">Demander une démo</a>
            </li>
          </ul>
        </div>
      </div>
    </nav><!--navbar end-->
    <section class="hero-static-image">
      <div class="container">
        <div class="row">
          <div class="col-md-10 offset-md-1 text-center">
            <h1>{{title_banner}}</h1>
            <p class="lead margin-b-30">
              {{text_banner}}
            </p>
            <div class="buttons">
              <a href="{{lien_bouton_banner}}" class="btn btn-xl btn-primary">{{text_bouton_banner}}</a>
            </div>
          </div>
        </div>
      </div>
      <img data-src="{{screenshot_banner}}" alt="Eventmaker bannière" class="screenshot-banner img-responsive center-img">
    </section>
    <!--intro section end-->
    <!-- <div class="container"> -->
    {% if notice %}<div class="alert alert-success">{{ notice }}</div>{% endif %}
    {% if alert %}<div class="alert alert-danger">{{ alert }}</div>{% endif %}
    {{ content }}
    <!-- </div> -->
    <div class='container' id="references">
      <div class="space-90"></div>
      <div class="center-title">
        <h2>{{title_reference}}</h2>
        {{text_reference}}
      </div>
      <ul class="row list-unstyled partners-list">
        {% assign sorted_all_articles = collections.articles | sort:'rank' %}
        {% assign count_reference = 0 %}
        {% for article in sorted_all_articles %}
          {% if article.type == "cas_clients" %}
            <li class="col-md-2 col-sm-4 col-6">
              <img data-src="{{article.illustration}}" class="img-fluid" alt="{{article.titre}}">
            </li>
            {% assign count_reference = count_reference | plus: 1 %}
            {% if count_reference >= 48 %}
              {% break %}
            {% endif %}
          {% endif %}
        {% endfor %}
      </ul>
    </div>

    <div class="space-70"></div>
    <footer class="footer">
      <div class="space-50"></div>
      <div class="container">
        <div class="row vertical-align-child-top">
          <div class="col-md-5 margin-b-30">
            <div class="col-sm-12 col-md-10" style="padding:0;">{{text_footer}}</div>
            <a href="/home">
                <img data-src="{{ 'eventmaker_blanc_transparent.png' | image_url }}" alt="Eventmaker logo" class="logo">
              </a>
          </div>
          <div class="col-md-2 col-6">
            <ul class="list-unstyled">
              {% for item in menus.menu_principal %}
              {% if forloop.index <= 4 %}
                <li><a href="{{item.url}}">{{ item.name }}</a></li>
              {% endif %}
              {% endfor %}
            </ul>
          </div>
          <div class="col-md-2 col-6">
            <ul class="list-unstyled">
              {% for item in menus.menu_footer %}
              {% if forloop.index > 4 %}
                <li><a href="{{item.url}}">{{ item.name }}</a></li>
              {% endif %}
              {% endfor %}
              <li><a href="{{baseurl}}/en" title="English">EN</a></li>
            </ul>
          </div>
          <div class="col-md-3">
            <ul class="list-inline social">
              {% if facebook != blank %}
                <li class="list-inline-item"><a href="{{facebook}}" target="_blank"><i class="ion-social-facebook"></i></a></li>
              {% endif %}
              {% if twitter != blank %}
                <li class="list-inline-item"><a href="{{twitter}}" target="_blank"><i class="ion-social-twitter"></i></a></li>
              {% endif %}
              {% if googleplus != blank %}
                <li class="list-inline-item"><a href="{{googleplus}}" target="_blank"><i class="ion-social-googleplus"></i></a></li>
              {% endif %}
              {% if linkedin != blank %}
                <li class="list-inline-item"><a href="{{linkedin}}" target="_blank"><i class="ion-social-linkedin"></i></a></li>
              {% endif %}
              {% if rss != blank %}
                <li class="list-inline-item"><a href="{{rss}}" target="_blank"><i class="ion-social-rss"></i></a></li>
              {% endif %}
              {% if youtube != blank %}
                <li class="list-inline-item"><a href="{{youtube}}" target="_blank"><i class="ion-social-youtube"></i></a></li>
              {% endif %}
              {% if instagram != blank %}
                <li class="list-inline-item"><a href="{{instagram}}" target="_blank"><i class="ion-social-instagram"></i></a></li>
              {% endif %}
            </ul>
          </div>
        </div>
        <div class="row col-md-12 sub-footer">
      <span>{{copyright}}</span>
      <div>
      {% for item in menus.menu_sub_footer %}
              <a href="{{item.url}}">{{ item.name }}</a>
      {% endfor %}
      </div>
      </div>
      </div>
      <div class="space-20"></div>
    </footer>
    <div class="cookie fixed-bottom">{{text_cookie}} <a href="{{baseurl}}/{{lien_cookie}}"><u>{{text_lien_cookie}}</u></a> <a class="btn btn-primary">OK</a></div>
